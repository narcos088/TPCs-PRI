extends layout

block content
  .w3-container
    .w3-card-4
      header.w3-container.w3-teal
        h3 MyPara (nodeJS)
      .w3-container 
        form.w3-container
          label.w3-text-teal
              b Ficheiro
          input.w3-input.w3-border.w3-light-grey(type="file", id='ficheiro')
          label.w3-text-teal
              b Descrição
          input.w3-input.w3-border.w3-light-grey(type="text", id='desc')
          input.w3-btn.w3-blue-grey(id="submit", value='Enviar')
        hr  
        .w3-container
          table.w3-table-all(id='tabela')  
      footer.w3-container.w3-teal
        address Gerado por AjaxApp
