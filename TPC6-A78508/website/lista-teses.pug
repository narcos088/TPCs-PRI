extends layout
block content 
    .w3-container   
        .w3-card-4  
            header.w3-container.w3-teal
                h3 Lista de Teses
            .w3-container
                table.w3-table-all
                    tr
                        th Nome
                        th Número
                        th Curso
                        th Instituição
                        th Título
                        th Data
                        th Tipo
                    each tese in lista 
                        tr 
                            td= tese.nome
                            td= tese.numero
                            td= tese.curso
                            td= tese.instituicao
                            td= tese.titulo
                            td= tese.data
                            td= tese.tipo
            br
            .w3-container   
                a.w3-button.w3-border-teal.w3-border.w3-round-large(href="http://localhost:4006/") Home
            br
            footer.w3-container.w3-teal
                address Gerado por formApp
